  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_OPTIONS='-r dotenv/config' nest start --watch | pino-pretty",
    "start:dev-old": "set NODE_OPTIONS=-r dotenv/config && nest start --watch | pino-pretty",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "ts-node src/modules/seed/seed.ts; node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-imports": "prettier --write . --plugin=prettier-plugin-organize-imports",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compose:local": "docker compose -f docker-compose-local.yml up",
    "seed": "ts-node src/modules/seed/seed.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": "prettier --write . --plugin=prettier-plugin-organize-imports"
  },
